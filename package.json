{
  "name": "nubis-sdk",
  "version": "1.0.3",
  "description": "Official Nubis SDK for JavaScript and TypeScript",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.esm.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "nubis",
    "cloud",
    "infrastructure",
    "sdk",
    "api"
  ],
  "author": "Nubis Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/usenubis/sdk-js.git"
  },
  "dependencies": {
    "axios": "^1.13.2"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.9.0"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  }
}